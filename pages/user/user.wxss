.user {
  margin: 0;
  padding: 0;
  background: #f5f5f5;

  width: 100%;
}
.dc{
   display: flex;
  justify-content: center;
  align-items: center;

}
.user .username {
  background: none;
  height: 191.1rpx;
  width: 100%;
  position: relative;
  z-index: 5;
}

.user .username .bg image {
  position: absolute;
  background: none;
  z-index: -1;
  width: 100%;
  height: 191.1rpx;
}

.user .usercont {
  height: 98rpx;
  width: 100%;
  position: relative;
}

.user .usercont .img {
  display: inline-block;
  position: absolute;
  top: 46rpx;
  left: 26rpx;
  height: 98rpx;
  width: 98rpx;
  border-radius: 50%;
}

.user .usercont .img image {
  width: 100%;
  height: 100%;
}

.user .usercont .text {
  width: 200rpx;
  height: 100%;
  position: absolute;
  left: 154rpx;
  top: 46rpx;
  display: flex;
  align-items: flex-start;
  flex-direction: column;
  color: white;
}

.user .usercont .text .name {
  font-size: 32rpx;
}

.user .usercont .text .id {
  font-size: 28rpx;
  opacity: 0.9;
}

.allorders {
  margin-top: 19rpx;
  width: 100%;
  height: 230rpx;
  background: white;
}

.allorders .oderhead {
  width: 100%;
  height: 71rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.allorders .oderhead .ot {
  display: flex;
  height: 70rpx;
  width: 100%;
  justify-content: space-between;
  align-items: center;
}

.allorders .oderhead .ot text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.allorders .oderhead .ot image {
  width: 18rpx;
  height: 29rpx;
}

.allorders .oderhead .ob {
  width: 100%;
  height: 1px;
  background: rgba(0, 0, 0, 1);
  opacity: 0.1;
}

.allorders .odercont {
  width: 100%;
  height: 159rpx;
  display: flex;
}

.allorders .odercont .cont {
  width: 150rpx;
  height: 100%;
  display: flex;
  flex-direction: row-reverse;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 28rpx 0 0 0;
 
}
.allorders .odercont .cont  image{
 width: 50rpx;
 height: 40rpx;
}
.m20{
  margin-top: 20rpx;
}
.allorders .odercont .cont  text{
 width: 100%;
 text-align: center;
 font-size:26rpx;
font-family:PingFang SC;
font-weight:400;
color:rgba(68,68,68,1);
}
