 page {
  background-color: #f5f5f5;
}
.seaches{
  margin: 0;
  padding: 0;
  background: #f5f5f5;
  width: 100%;
  height: 100%;
  box-sizing: border-box;



  
}

.search_box {
 
  display: flex;

  height: 108rpx;

  border-top: 1rpx solid #f5f5f5;

  background: #fff;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;

  background: #fff;
  
}

.search_box .search {
  border-style: solid;
  border-width: 2rpx;
  border-color: #f84e67;
  border-radius: 10rpx;
  /* margin-left: 30rpx; */
  overflow: hidden;
  flex: 1;
  display: flex;
}

.search_box .search .search_logo {
  float: left;
  /* padding: 14rpx 12rpx 14rpx 30rpx; */
  padding: 14rpx 12rpx 14rpx 30rpx;
  width: 40rpx;
  height: 32rpx;
}

.search_box .search .search_logo image {
  display: block;
  width: 40rpx;
  height: 32rpx;
}

.search_box .search input {
  height: 60rpx;
  width: 100%;
  font-size: 29rpx;
  margin-left: 20rpx;
}

.search_box .search_button {
width:100rpx;
height:70rpx;
background:rgba(240,43,80,1);
border-radius:10rpx;
  margin-left: 10rpx;
  display: flex;
  justify-content: center;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  align-items: center;
 
  /* padding: 0 30rpx 0 10rpx; */
}
.checktbs{
height: 99rpx;
box-sizing: border-box;
padding-top: 10rpx;
background: #fff;

}
.search_box .search_button image {
  width: 100%;
  height: 100%;
}
   .indent.checktbs {
   height: 90rpx;
   width: 100%;
 }

 .checktbs .tabs {
   width: 100%;
  
   height: 91rpx;
   display: flex;
   background: #fff;
   align-items: center;
   padding-bottom: 8rpx;
   box-sizing: border-box;
   border: none!important;
 }
 .content{
   width: 100%;
   height: 999rpx;
   overflow: hidden;
   background: #f5f5f5;
   position: relative;

 }
 .content .imgs{
