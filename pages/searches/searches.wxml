<view class="seaches">
  <view class="search_box">
    <view class="search">
      <view class="search_logo">
        <image mode="widthFix" src="../../static/imgs/搜索.png"></image>
      </view>
      <input placeholder="面膜" bindtap="handurl" data-url="/pages/search/search"></input>
    </view>
    <view class="search_button">
      取消
    </view>
  </view>
  <view class="checktbs">
    <i-tabs current="{{ current }}" i-class="tabs" color="#F02B50" bindchange="handleChange" i-class="tabs">
      <i-tab key="tab1" title="全部"></i-tab>
      <i-tab key="tab2" title="价格从低到到高"></i-tab>
      <i-tab key="tab3" title="价格从高到低"></i-tab>
     
    </i-tabs>
  </view>

  <view class="content">
    <view class="imgs">
        <image src="../../static/imgs/组 1 (1).png"></image>
        <!-- <image class="img" src="../../static/imgs/搜索无结果！.png"></image> -->
        <text>搜索无结果！</text>
    
    </view>
  
  </view>
</view>