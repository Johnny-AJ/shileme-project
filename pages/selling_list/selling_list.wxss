/* pages/selling_list/selling_list.wxss */
page{
background-color: #f5f5f5;
}
.bgc {
width: 100%;
position: relative;


}

/* tab */
.bgc .obc{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 268rpx;
  z-index:-1;
}.bgc .obc image{
  width: 100%;
  height: 100%
}
.checktbs {
  height: 90rpx;
  width: 100%;

}

.checktbs  .conts {
  
  min-width: 750rpx;
  height: 100%;
  padding-left: 20rpx;
  display: flex;
  justify-content: space-between;
}

.checktbs  .conts .cont {
  float: left;
  width: 150rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.checktbs  .conts .cont text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  
  padding-bottom: 17rpx;
}
 .checktbs  .conts .cont .color{
   font-size: 30rpx;
   font-weight: bold;
   border-bottom: 4rpx #fff solid;
 }
/* 急救 */

.conts {
  width: 100%;
  margin-top: 0;

  display: flex;
  justify-content: center;
  flex-wrap: wrap;

}

.cons_box {
  width: 730rpx;
  height: 250rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  display: flex;
  margin-top: 20rpx;
}

.cons_box .imgs image {
  width: 200rpx;
  height: 200rpx;
  /* border: 1px solid red; */
  padding: 25rpx 29rpx 25rpx 20rpx;
}

.navd {
  padding-top: 25rpx;
}

.cons_box .d1 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
  padding-bottom: 6rpx;
}

.cons_box .d2 {
  width: 438rpx;
  height: 62rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  padding-bottom: 12rpx;
}

.i-tabs-tab-title-current {
  font-size: 40rpx !important;
}

.cons_box .d3 {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(240, 43, 80, 1);
  padding-bottom: 12rpx;
}

.cons_box .d4 {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
}
