/* pages/selling_list/selling_list.wxss */

.bgc {
  width: 750rpx;
  height: 1780rpx;
  overflow: hidden;
  background-color: skyblue;
  /* background-color: #f5f5f5; */
}

/* tab */

.indent.checktbs {
  height: 90rpx;
  width: 100%;
}

.indent.tabs {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  height: 91rpx;
  display: flex;
  background: #fff;
  text-align: center;
  padding: 10rpx;
  box-sizing: border-box;
  border: none !important;
}

/* 急救 */

.cons_box {
  width: 730rpx;
  height: 250rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  margin: 20rpx 10rpx;
  display: flex;
}

.cons_box .imgs image {
  width: 200rpx;
  height: 200rpx;
  /* border: 1px solid red; */
  padding: 25rpx 29rpx 25rpx 20rpx;
}

.navd {
  padding-top: 25rpx;
}

.cons_box .d1 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
  padding-bottom: 6rpx;
}

.cons_box .d2 {
  width: 438rpx;
  height: 62rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  padding-bottom: 12rpx;
}

.cons_box .d3 {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(240, 43, 80, 1);
  padding-bottom: 12rpx;
}

.cons_box .d4 {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
}
