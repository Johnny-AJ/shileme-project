.comment {
  width: 100%;
  height: 490rpx;
  box-sizing: border-box;
  padding: 0 0 0 30rpx;
  margin-top: 20rpx;
  position: relative;
  background: #fff;
}

.comment .title {
  height: 80rpx;
  width: 690rpx;
  /* background: red */
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.comment .title .conts {
  width: 300rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.comment .title  .more {
  width: 132rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(240, 43, 80, 1);
  display: flex;
  align-items: center;
}

.comment .title  .more image {
  width: 14rpx;
  height: 23rpx;
  margin-left: 13rpx;
}

.comment  .pj {
  height: 409rpx;
  position: absolute;
  left: 0rpx;
}

.comment  .pj .ttop {
  height: 169rpx;
  width: 690rpx;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.comment  .pj .ttop view {
  margin: 20rpx 0 0 20rpx;
  display: inline-block;
  background: rgba(254, 231, 231, 1);
  border-radius: 5px;
  padding: 14rpx 26rpx 14rpx 28rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
}

.comment  .pj  .tbottom {
  width: 750rpx;
  padding-top: 20rpx;
  overflow-x: auto;
  height: 202rpx;
  overflow-y: hidden;
}

.comment  .pj  .tbottom::-webkit-scrollbar {
  display: none;
}

.comment  .pj  .tbottom  .tblog {
  width: 3130rpx;
  height: 200rpx;
}

.comment  .pj  .tbottom .pl {
  float: left;
  width: 606rpx;
  height: 200rpx;
  box-sizing: border-box;
  margin-left: 20rpx;
  /* border: 1rpx solid #f5f5f5; */
  position: relative;
}

.comment  .pj  .tbottom .pl:nth-child(1) {
  margin-left: 0;
}

.comment  .pj  .tbottom .pl .head {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 56rpx;
  height: 56rpx;
}

.comment  .pj  .tbottom .pl .head image {
  width: 100%;
  height: 100%;
}

.comment  .pj  .tbottom .pl .cont {
  width: 290rpx;
  height: 100%;
  box-sizing: border-box;
  padding-top: 10rpx;
  position: absolute;
  left: 97rpx;
}

.comment  .pj  .tbottom .pl .cont text {
  width: 100%;
  display: inline-block;
  line-height: 10rpx;
}

.comment  .pj  .tbottom .pl .cont .name {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
}

.comment  .pj  .tbottom .pl .cont .time {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  position: relative;
  top: -23rpx;
  color: rgba(102, 102, 102, 1);
}

.comment  .pj  .tbottom .pl .cont .textcont {
  height: 67rpx;
  width: 100%;
  position: relative;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
  top: -23rpx;
}

.comment  .pj  .tbottom .pl .imsg {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  right: 0;
}

.comment  .pj  .tbottom .pl .imsg  text {
  position: absolute;
  display: inline-block;
  width: 60rpx;
  height: 30rpx;
  right: 10rpx;
  bottom: 10rpx;
  font-size: 22rpx;
  text-align: center;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  background: rgba(0, 0, 0, 1);
  opacity: 0.39;
}

.comment  .pj  .tbottom .pl   image {
  width: 100%;
  height: 100%;
}

