.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.75);
  display: none;
}

.zan-dialog__container {
  position: fixed;
  bottom: 0rpx;
  width: 750rpx;
  /* height: 600rpx; */
  background: #f8f8f8;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  box-shadow: 0px 3px 3px 2px gainsboro;
  padding-bottom: 30rpx;
}

.zan-dialog__container .conts {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
  background: #fff;
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}

.zan-dialog--show1 .zan-dialog__container {
  transform: translateY(0);
  background: #fff;
}

.zan-dialog--show1 .zan-dialog__mask {
  display: block;
}
.show1 .zan-dialog__container {
  transform: translateY(0);
  background: #fff;
}

.show1 .zan-dialog__mask {
  display: block;
}

.show1 .zan-dialog__container .conts {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.zan-dialog__container .conts .title {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.zan-dialog__container .conts .coupon {
  width: 100%;
  border-top: 1rpx solid rgba(8, 8, 8, 0.2);
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  max-height: 800rpx;
  overflow: auto;
  padding-bottom: 30rpx;
}

