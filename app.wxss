/**app.wxss**/
@import './weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page, view, swiper, swiper-item, navigator, image, button, input, text {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

image {
  width: 100%;
}

